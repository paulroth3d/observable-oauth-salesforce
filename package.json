{
  "name": "observable-oauth-salesforce",
  "version": "0.3.0",
  "description": "Simple server that provides oAuth 2.0 access to salesforce from Observable",
  "engines": {
    "node": "14.x"
  },
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "debug": "node --inspect-brk src/index.js",
    "debug:remote": "node --inspect src/index.js",
    "lint": "node node_modules/eslint-watch/bin/esw .",
    "lint:watch": "npm run lint -- --watch src test",
    "test": "node node_modules/jest/bin/jest .",
