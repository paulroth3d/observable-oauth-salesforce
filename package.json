    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "path": "^0.12.7",
    "pino": "^6.11.2",
    "salesforce-oauth-express-middleware": "^1.0.2"
  },
  "devDependencies": {
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-watch": "^7.0.0",
    "got": "^11.3.0",
    "jest": "^26.6.3",
    "tape": "^4.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/heroku/node-js-getting-started"
